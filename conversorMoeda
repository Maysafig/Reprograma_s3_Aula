//Crie uma função que irá converter uma quantia de real para dolar utilizando a cotação do dia.
//cotação dólar hoje = U$ 4.68

function calculaRealParaDolar(real){
    let cotacaoDoDia = 4.68
    let resultadoEmDolar = real / cotacaoDoDia
    return `Real: ${real} para dólar : ${resultadoEmDolar.toFixed(2)}`
}

console.log(calculaRealParaDolar(1000)) // 0,21
